<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prueba.admin.AdminController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ButtonBar prefHeight="40.0" prefWidth="200.0">
      <buttons>
         <Label text="Admin login" />
      </buttons></ButtonBar>
   <TextField fx:id="adminUsernameField" promptText="Username" />
   <PasswordField fx:id="adminPasswordField" promptText="Password" />

    <Label fx:id="welcomeText" />
    <Button alignment="CENTER" focusTraversable="false" onAction="#onHelloButtonClick" text="Hello world"/>
    <Button alignment="CENTER" focusTraversable="false" onAction="#LoginAdminHome" text="Log in"/>
</VBox>