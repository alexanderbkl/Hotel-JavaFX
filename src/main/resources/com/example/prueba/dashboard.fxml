<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prueba.DashboardController">
    <JFXButton/>
    <BorderPane fx:id="borderpane" prefHeight="600.0" prefWidth="1000.0">
        <left>
            <AnchorPane prefHeight="570.0" prefWidth="140.0" BorderPane.alignment="CENTER">
                <VBox prefHeight="569.0" prefWidth="140.0">
                    <JFXButton onAction="#AdminHome" prefHeight="25.0" prefWidth="140.0" text="Home">
                        <graphic>
                        </graphic>
                        <padding>
                            <Insets right="44.0"/>
                        </padding>
                    </JFXButton>
                    <JFXButton layoutX="10.0" layoutY="10.0" onAction="#AddEmployee" prefHeight="25.0"
                               prefWidth="140.0" text="Add Employee">
                        <graphic>
                        </graphic>
                    </JFXButton>
                    <JFXButton layoutX="10.0" layoutY="35.0" onAction="#AdminEmployeeInfo" prefHeight="25.0"
                               prefWidth="140.0" text="Employee Info">
                        <graphic>
                        </graphic>
                    </JFXButton>
                    <JFXButton layoutX="10.0" layoutY="60.0" onAction="#AdminCustomerInfo" prefHeight="25.0"
                               prefWidth="140.0" text="Customer Info">
                        <graphic>
                        </graphic>
                    </JFXButton>
                    <JFXButton layoutX="10.0" layoutY="85.0" onAction="#AdminEarnLog" prefHeight="25.0"
                               prefWidth="140.0" text="Earning Log">
                        <graphic>
                        </graphic>
                    </JFXButton>
                    <JFXButton layoutX="10.0" layoutY="110.0" onAction="#AdminTotalEarnings" prefHeight="25.0"
                               prefWidth="140.0" text="Total Earnings">
                        <graphic>
                        </graphic>
                    </JFXButton>
                </VBox>
            </AnchorPane>
        </left>
        <top>
            <AnchorPane prefHeight="30.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <Label text="Admin Dashboard"/>
                </HBox>
                <HBox layoutX="900.0" prefHeight="100.0" prefWidth="200.0">
                </HBox>
            </AnchorPane>
        </top>
    </BorderPane>
</AnchorPane>
