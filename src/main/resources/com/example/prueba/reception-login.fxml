<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0"  xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.prueba.reception.ReceptionLoginController">
<padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
</padding>
<ButtonBar prefHeight="40.0" prefWidth="200.0">
    <buttons>
        <Label text="Reception login" />
    </buttons></ButtonBar>
<TextField fx:id="receptionUsernameField" promptText="Username" />
<PasswordField fx:id="receptionPasswordField" promptText="Password" />

<Button alignment="CENTER" focusTraversable="false" onAction="#LoginReceptionHome" text="Log in"/>
</VBox>